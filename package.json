{
    "name": "logbook",
    "type": "module",
    "version": "0.0.0",
    "private": true,
    "scripts": {
	"start": "cross-env DEBUG=notes:* node --experimental-modules ./app",
	"server1": "cross-env DEBUG=notes:* PORT=3001 node ./app",
	"server2": "cross-env DEBUG=notes:* PORT=3002 node ./app",
	"fs-start": "cross-env DEBUG=notes:* PORT=3000 NOTES_MODEL=fs node ./app.js",
	"fs-server1": "cross-env NOTES_MODEL=fs PORT=3001 node ./app.js",
	"fs-server2": "cross-env NOTES_MODEL=fs PORT=3002 node ./app.js",
	"level-start": "cross-env DEBUG=notes:* PORT=3000 NOTES_MODEL=level node ./app.js",
	"sqlite3-setup": "sqlite3 chap07.sqlite3 --init models/schema-sqlite3.sql",
	"sqlite3-start": "cross-env SQLITE_FILE=chap07.sqlite3 DEBUG=notes:*NOTES_MODEL=sqlite3 node ./app.js",
	"sqlite3-server1": "cross-env SQLITE_FILE=chap07.sqlite3 NOTES_MODEL=sqlite3 PORT=3001 node ./app.js",
	"sqlite3-server2": "cross-env SQLITE_FILE=chap07.sqlite3 NOTES_MODEL=sqlite3 PORT=3002 node ./app.js",
	"sequelize-start": "cross-env DEBUG=notes:* SEQUELIZE_CONNECT=models/sequelize-sqlite.yaml NOTES_MODEL=sequelize node ./app.js",
	"sequelize-server1": "cross-env SEQUELIZE_CONNECT=models/sequelize-sqlite.yaml NOTES_MODEL=sequelize PORT=3001 node ./app.js",
	"sequelize-server2": "cross-env SEQUELIZE_CONNECT=models/sequelize-sqlite.yaml NOTES_MODEL=sequelize PORT=3002 node ./app.js",
	"mongodb-start": "cross-env DEBUG=notes:* MONGO_URL=mongodb://localhost/ MONGO_DBNAME=chap07 NOTES_MODEL=mongodb node ./app.js",
	"mongodb-server1": "cross-env DEBUG=notes:* MONGO_URL=mongodb://localhost/ MONGO_DBNAME=chap07 NOTES_MODEL=mongodb PORT=3001 node ./app.js",
"mongodb-server2": "cross-env DEBUG=notes:* MONGO_URL=mongodb://localhost/ MONGO_DBNAME=chap07 NOTES_MODEL=mongodb PORT=3002 node ./app.js"
    },
    "dependencies": {
	"cookie-parser": "~1.4.4",
	"debug": "~2.6.9",
	"express": "^4.16.4",
	"fs-extra": "^9.0.1",
	"hbs": "~4.0.4",
	"http-errors": "~1.6.3",
	"js-yaml": "^3.14.1",
	"level": "^6.0.1",
	"mongodb": "^3.6.3",
	"morgan": "~1.9.1",
	"mysql": "^2.18.1",
	"postgres": "^1.0.2",
	"sequelize": "^6.3.5",
	"sqlite": "^4.0.18"
    }
}
